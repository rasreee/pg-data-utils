{
  "version": "1.8.10",
  "compilerOptions": {
      "target": "es5",
      "sourceMap": false,
      "declaration": true,
      "module": "es6",
      "removeComments": false,
      "outDir": "../dist/",
      "noImplicitAny": true,
      "strict": true,
      "moduleResolution": "node",
      "experimentalDecorators": true,
      "useDefineForClassFields": true,
      "typeRoots": ["../node_modules/@types"],
      "lib": [
          "dom",
          "es2015",
          "scripthost",
          "es2015.promise",
          "es2015.generator",
          "es2015.iterable"
      ]
  },
  "exclude": ["node_modules"]
}
