{
  "name": "pg-data-utils",
  "author": "Lesley Chang",
  "repository": {
    "type": "git",
    "url": "https://github.com/rasreee/pg-data-utils.git"
  },
  "bugs": {
    "url": "https://github.com/rasreee/pg-data-utils/issues"
  },
  "version": "1.0.0",
  "main": "dist/pg-data-utils.cjs",
  "module": "dist/pg-data-utils.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "license": "MIT",
  "engines": {
    "node": ">=0.11"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "clean": "rimraf --no-glob ./dist ./tsconfig.tsbuildinfo ./.eslintcache && yarn cache clean",
    "build": "yarn clean && cross-env NODE_ENV=production microbundle --tsconfig ./tsconfig.json",
    "release": "yarn test:all && yarn build && yarn npm publish",
    "lint": "eslint --ignore-path .gitignore --cache --ext .js,.jsx,.ts,.tsx ./",
    "format": "yarn run lint --fix",
    "type-check": "rimraf ./tsconfig.tsbuildinfo && tsc --pretty --noEmit",
    "test": "jest --clearCache && NODE_ENV=test jest",
    "test:watch": "yarn test --watch",
    "test:all": "yarn run type-check && yarn test"
  },
  "devDependencies": {
    "@types/jest": "^27.4.0",
    "@types/node": "^14.6.3",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.4.7",
    "microbundle": "^0.14.2",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.3",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "date-fns": "^2.28.0",
    "tiny-invariant": "^1.2.0"
  },
  "packageManager": "yarn@3.1.1"
}
